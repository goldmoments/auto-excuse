local dmsgsi = dmsgs.deathmessages
print("started")
while wait() do
if game.Workspace[game.Players.LocalPlayer.Name].Humanoid.Health == 0 then
local s1 = dmsgsi[math.random(1, #dmsgsi)]
local s2 = "All"
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(s1, s2)
wait(game.Players.RespawnTime)
end
end
